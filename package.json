{
  "name": "@adaburrows/table-web-component",
  "description": "A very flexible table written as a Lit-based web component. Sorting capabilities and sensible defaults. Can be used in a headless mode for flexibility.",
  "version": "0.9.0",
  "type": "module",
  "module": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./table": {
      "types": "./types/table.d.ts",
      "import": "./dist/table.js"
    },
    "./react-table": {
      "types": "./types/react-table.d.ts",
      "import": "./dist/react-table.js"
    },
    "./table-store": {
      "types": "./types/table-store.d.ts",
      "import": "./dist/table-store.js"
    },
    "./table-context": {
      "types": "./types/table-context.d.ts",
      "import": "./dist/table-context.js"
    },
    "./table-context-element": {
      "types": "./types/table-context-element.d.ts",
      "import": "./dist/table-context-element.js"
    },
    "./react-table-context-element": {
      "types": "./types/react-table-context-element.d.ts",
      "import": "./dist/react-table-context-element.js"
    },
    "./field-definitions": {
      "types": "./types/field-definitions.d.ts",
      "import": "./dist/field-definitions.js"
    }
  },
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adaburrows/table-web-component.git"
  },
  "keywords": [
    "UI",
    "table"
  ],
  "author": "Jillian Ada Burrows",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/adaburrows/table-web-component/issues"
  },
  "dependencies": {
    "@lit-labs/context": "^0.2.0",
    "@lit-labs/react": "^1.1.1",
    "@lit-labs/scoped-registry-mixin": "^1.0.1",
    "@webcomponents/scoped-custom-element-registry": "^0.0.7",
    "lit": "^2.2.0",
    "lit-svelte-stores": "^0.2.1",
    "svelte": "^3.44.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "react": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.4"
  }
}
